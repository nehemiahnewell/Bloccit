<h1>Questions</h1>
<div class="row">
  <div class="col-md-8">
    <% @questionss.each do |question| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to question.title, question %>
          </h4>
          <small>
            <%= question.body %>
            <%= question.resolved %>
          </small>
        </div>
      </div>
    <% end %>
  </div>
  <div class="col-md-4">
    <%= link_to "New Question", new_question_path, class: 'btn btn-success  ' %>
  </div>
</div>